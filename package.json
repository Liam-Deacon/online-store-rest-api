    "license": "GNU Public License v3.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/liam-deacon/online-store-example.git"
    },
    "devDependencies": {
        "swagger2openapi": "^7.0.2"
    },
    "scripts": {
        "apispec": "wget -O apispec.json http://localhost:5000/apispec_1.json",
        "preswag2oas": "sed -i 's|\"/terms\"|\"http://localhost:5000/terms\"|' apispec.json",
        "swag2oas": "node_modules/.bin/swagger2openapi -o openapi_spec.yml apispec.json",
        "dev": "cd online_store/frontend && npm run dev",
        "start": "cd online_store/frontend && npm run dev",
        "unit": "cd online_store/frontend && jest --config test/unit/jest.conf.js --coverage",
        "e2e": "cd online_store/frontend && node test/e2e/runner.js",
        "test": "cd online_store/frontend && run unit && npm run e2e",
        "lint": "cd online_store/frontend && eslint --ext .js,.vue src test/unit test/e2e/specs",
        "build": "cd online_store/frontend && node build/build.js"
    }
}
